{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.o": true
	},
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": true,
	"explorer.fileNesting.patterns": {
		"*.c": "${capture}.h"
	},
	"cmake.configureOnOpen": false
}